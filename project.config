[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	create = group Registered Users
	create = group user/trautamaki (trautamaki)
	push = group Registered Users
	label-Verified = -1..+1 group Registered Users
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/trautamaki (trautamaki)
	label-Verified = -1..+1 group user/trautamaki (trautamaki)
	push = +force group user/trautamaki (trautamaki)
	read = group user/trautamaki (trautamaki)
	rebase = group user/trautamaki (trautamaki)
	removeReviewer = group user/trautamaki (trautamaki)
	submit = group user/trautamaki (trautamaki)
[access "refs/heads/*"]
	create = group Registered Users
	push = +force group user/trautamaki (trautamaki)
	pushMerge = group user/trautamaki (trautamaki)
	read = group user/trautamaki (trautamaki)
[access "refs/to/heads"]
	push = group Registered Users
